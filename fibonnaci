
package jurnal1_c;
import java.util.Scanner;

public class Jurnal1_c {

    
    public static void main(String[] args) {
        int ab = 0;
        int bc = 1;
        int cd = 0;
        int hasil = 0;
        
        Scanner sc = new Scanner(System.in);
	System.out.print("Masukkanpa deret fibonacci ? ");
	cd = sc.nextInt();
	for(int i=1;i<=cd;i++){
            ab=bc;
            bc=hasil;
            System.out.print(hasil+" ");
            hasil=ab+bc;
    }   
    }
